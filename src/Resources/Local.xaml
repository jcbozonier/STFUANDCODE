<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <RadialGradientBrush GradientOrigin="0.5, 0.5" RadiusX="0.5" RadiusY="0.5" x:Key="Parse">
        <GradientStop Color="Green" Offset="0.0" />
        <GradientStop Color="LightGreen" Offset="1.0" />
    </RadialGradientBrush>
    <RadialGradientBrush GradientOrigin="0.5, 0.5" RadiusX="0.5" RadiusY="0.5" x:Key="NoParse">
        <GradientStop Color="DarkRed" Offset="0.0" />
        <GradientStop Color="LightSalmon" Offset="1.0" />
    </RadialGradientBrush>
</ResourceDictionary>